{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Launch App",
      "skipFiles": ["<node_internals>/**"],
      "port": 9229,
      "sourceMapPathOverrides": {
        "webpack:///./~/*": "${workspaceRoot}/node_modules/*",
        "webpack:///./*": "${workspaceRoot}/subfolder/*",
        "webpack:///*": "*"
      }
    }
  ]
}
